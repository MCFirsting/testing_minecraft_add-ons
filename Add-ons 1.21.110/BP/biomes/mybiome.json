{
    "format_version": "1.21.110",
    "minecraft:biome": {
        "description": {
            "identifier": "testing:mybiome"
        },
        "components": {
            //กำหนดวัสดุที่ใช้สร้างภูมิประเทศ
            "minecraft:surface_builder": {
                "builder": {
                    "type": "minecraft:overworld",
                    //ความลึกของพื้นทะเล
                    "sea_floor_depth": 3,
                    //วัสดุชั้นบน
                    "top_material": "minecraft:moss_block",
                    //วัสดุชั้นกลาง
                    "mid_material": "minecraft:clay",
                    //วัสดุรองพื้น
                    "foundation_material": "minecraft:mud",
                    //วัสดุที่เป็นของเหลว
                    "sea_material": "minecraft:lava",
                    //วัสดุพื้นใต้น้ำ
                    "sea_floor_material": "minecraft:lava"
                }
            },
            //การแทนที่ไบโอม
            "minecraft:replace_biomes": {
                "replacements": [
                    {
                        "dimension": "minecraft:overworld",
                        //ระบุชื่อไบโอมเดิมที่จะถูกแทนที่
                        "targets": [
                            "minecraft:beach",
                            "minecraft:plains"
                        ],
                        //โอกาสในการแทนที่
                        "amount": 0.5,
                        //ขนาดความถี่
                        "noise_frequency_scale": 25
                    }
                ]
            }
        }
    }
}
